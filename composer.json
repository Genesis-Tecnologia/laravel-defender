{
  "name": "genesis-tecnologia/laravel-defender",
  "description": "Roles & Permissions for Laravel",
  "license": "MIT",
  "require": {
    "php": ">=7.2.0 || ^8.0",
    "laravel/framework": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0 || ^10.0",
    "friendsofphp/php-cs-fixer": "~2.11",
    "orchestra/testbench": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0",
    "orchestra/database": "^4.0 || ^5.0 || ^6.0 || ^7.0",
    "fakerphp/faker": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "GenesisTecnologia\\Defender\\": "src/Defender/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "minimum-stability" : "stable",
  "extra": {
    "laravel": {
      "providers": [
        "GenesisTecnologia\\Defender\\Providers\\DefenderServiceProvider"
      ],
      "aliases": {
        "Defender": "GenesisTecnologia\\Defender\\Facades\\Defender"
      }
    }
  }
}
